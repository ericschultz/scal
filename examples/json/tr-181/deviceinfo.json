		"default_type": "string",
		"parameters": {
			"DeviceLog": {
				"backend": "shell",
				"backend_data": {
					"command": [ "logread" ],
					"maxlen": 32768
				}
			},
			"SoftwareVersion": {},
			"Manufacturer": {},
			"ManufacturerOUI": {},
			"ModelName": {},
			"Description": {},
			"ProductClass": {},
			"SerialNumber": {},
			"HardwareVersion": {}
		}
	}]
]}
